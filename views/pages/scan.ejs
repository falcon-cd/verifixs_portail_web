                <div class="main-scan">
                    <div class="scan-card">
                      <div class="text">
                        <h2>SCAN QR CODE</h2>
                      </div>
                      <div id="cam-zone">
                        <div id="qr-reader" style="max-width:400px"></div>
                        <div id="qr-reader-results"></div>
                      </div>
                      
                      <div id="qr-zone"  class="qr-zone">
                        <img style="border-radius: 20px;" class="scan-image-placeholder img-fluid" src="assets/images/qr-scan-img.jpg"
                            onclick="goTocam()"  />
                      </div>
                      <script>
                        var qrZone = document.getElementById("qr-zone");
                        var camZone = document.getElementById("cam-zone");
                        var resultContainer = document.getElementById('qr-reader-results');
                        // var lastResult, countResults = 0;

                        function goTocam(){
                            html5QrcodeScanner.render(onScanSuccess);
                            qrZone.style.display = "none"
                            camZone.style.display = "block"  
                        }

                        
                    
                        function onScanSuccess(decodedText, decodedResult) {
                            if (decodedText) {
                                // ++countResults;
                                // lastResult = decodedText;
                                qrZone.style.display = "block"
                                camZone.style.display = "none" 
                                document.getElementById("html5-qrcode-button-camera-stop").click();
                                alert(decodedText) 
                                // Handle on success condition with the decoded message.
                                console.log(`Scan result ${decodedText}`, decodedResult);
                            }
                        }
                    

                        var html5QrcodeScanner = new Html5QrcodeScanner(
                            "qr-reader", { fps: 10, qrbox: 250 });
                    </script>
            
